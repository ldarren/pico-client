[
    [
    ],[
        "mod/ldarren/wm/desktop.css",
        "lib/lean/lean.css",
        "css/skeleton.css",
        "css/icon.css",
        "css/main.css"
    ],[
        ["owner","models",[]],
        ["data","models",[]],
        ["dataUsers","models",[]],
        [   
            "ld/auth/DataSync",
            "ctrl",
            [
                ["data","ref","data"],
                ["dataUsers","ref","dataUsers"],
                ["pull","models",{
                    "list":"cr/data/poll"
                }],
                ["freq","number",60000]
            ]
        ], [   
            "ld/auth/Session",
            "ctrl",
            [
                ["owner","ref","owner"],
                ["users","ref","data"],
                ["authPages","list",[]]
            ]
        ]
    ],{
        "*action":[
            [
                ["allModels", "refs", "models"],
                [
                    "ld/wm/Launcher",
                    "view",
                    [
                        ["allModels", "refs", "models"],
                        [
                            "mod/UserMgr",
                            "view",
                            [
                                ["id", "text", "users"],
                                ["icon", "text", "users"],
                                ["name", "text", "User"],
                                ["owner", "ref", "owner"],
                                ["data", "ref", "data"],
                                [
                                    "mod/Signin",
                                    "view",
                                    [
                                        ["owner", "ref", "owner"],
                                        ["auth", "models", {
                                            "create": "cr/user/signin"
                                        }]
                                    ]
                                ], [
                                    "mod/Signup",
                                    "view",
                                    []
                                ], [
                                    "mod/UserInfo",
                                    "view",
                                    [
                                        ["owner", "ref", "owner"],
                                        ["data", "ref", "data"]
                                    ]
                                ]
                            ]
                        ],
                        ["defaultPath", "url", "@json/default.json"],
                        ["userPath", "url", "@json/USER_ID.json"]
                    ]
                ],[
                    ["ld/wm/Desktop","ld/wm/mixin"],
                    "view",
                    [
                        ["apps","list",[]],
                        ["ld/wm/Window","view",[]],
                        ["ld/wm/MenuBar","view",[]],
                        ["ld/wm/File","view",[]]
                    ]
                ]
            ],{
            }
        ]
    }
]
