{
    "deps":{
        "util":"api/util",
        "user":"api/user",
        "room":"api/room",
        "web":["context","webServer"]
    },
    "routes":{
        "": [
            ["util","help"]
        ],
        "ERR": [
            ["web","error","query","res","error"]
        ],
        "ERR/pdl/stream": [
            ["web","SSEStop","res"]
        ],
        "END": [
            ["web","render","query","res"]
        ],
        "room.404": [
            ["web","SSEStop","res"]
        ],
        "web.dc":[
            ["room","remove","res"]
        ],
        "/pdl":[
            ["util","route","req"],
            ["web","parse","req","res"]
        ],
        "/pdl/stream":[
            ["user","verify","input","user"],
            ["user","poll","input"],
            ["room","add","res","user"],
            ["web","SSEStart","req","res"],
            ["room","stream",["poll"],"user"]
        ],
        "pdl/user/signin": [
            ["user","signin","input","users"],
            ["web","render","query","res"]
        ],
        "pdl/user/signup": [
            ["user","signup","input","users"],
            ["web","render","query","res"]
        ],
        "pdl/user/signout": [
            ["user","signout","input","users"],
            ["web","render","query","res"]
        ]
    }
}
