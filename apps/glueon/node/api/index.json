{
    "deps":{
        "util":"api/util",
        "chat":"api/chat",
        "user":"api/user",
        "directory":"api/directory",
		"stream":"api/stream",
        "web":["context","webServer"]
    },      
    "routes":{
        "/glueon":[
            ["util","route","req"],
			["web","parse","req","res"]
        ],
		"/glueon/stream":[
			["stream","prepare","cred","input","output"],
            ["user","verify","cred"],
			["user","poll","input","output"],
			["directory","poll","cred","input",":poll","output"],
			["stream","add","res","cred"],
			["web","SSEStart","req","res"],
			["stream","render",["poll"],"cred","output"]
		],
        "ERR|#msg": [
			["util","logParams"],
            []
        ],
        "END|#msg": [
            [] 
        ],
        "glueon/user/signout":[
            ["user","verify","cred"],
            ["user","removeSession","cred"],
            []
        ],
        "glueon/user/update":[
            ["user","verify","cred"],
            ["user","update","cred","input","output"],
            ["user","replyPrivate","output"],
            []
        ],
        "glueon/user/list":[
            ["user","verify","cred"],
            ["user","list","input",":output"],
            ["user","replyList",":output"],
            []
        ],
        "glueon/user/read":[
            ["user","verify","cred"],
            ["user","read","input","output"],
            ["user","reply","output"],
            []
        ],
		"glueon/directory/read":[
            ["util","have","input",[["cwd"]]],
            ["user","verify","cred"],
			["directory","read","cred","input","output"],
            ["directory","replyPrivate","output"],
			[]
		],
        "#help": [
            ["util","help"] 
        ] 
    }
}   
