{
    "deps":{
        "util":"api/util",
        "user":"api/user",
        "email":"api/email",
        "entity":"api/entity",
        "directory":"api/directory",
		"stream":"api/stream",
        "web":["context","webServer"]
    },      
    "routes":{
        "/jasy":[
            ["util","route","req"],
			["web","parse","req","res"]
        ],
		"/jasy/stream":[
			["stream","prepare","cred","input","output"],
            ["user","verify","cred"],
			["user","poll","input","output"],
			["directory","poll","cred","input",":poll","output"],
			["entity","last","input",":poll","output"],
			["stream","add","res","cred"],
			["web","SSEStart","req","res"],
			["stream","render",["poll"],"cred","output"]
		],
        "ERR|#msg": [
			["util","logParams"],
            []
        ],
        "END|#msg": [
            [] 
        ],
        "jasy/email/signup":[
            ["util","have","input",[["email","pwd"]]],
            ["email","verifyEmail","input",[null]],
            ["util","randomStr",["$verifyId",20]],
            ["email","sendConfirmation","cred","input","$verifyId","output"],
            ["user","replyPrivate","output"],
            []
        ],
        "jasy/email/confirm":[
            ["util","have","input",[["email","verifyId"]]],
            ["email","verifyEmail","input",[null]],
            ["email","confirmation","cred","input","user"],
            ["user","create","cred","user"],
            ["directory","newUser","cred","user"],
            ["directory","userJoin","cred","user","input"],
            ["util","randomStr",["$sessKey",20]],
            ["user","createSession","cred","user","$sessKey","session"],
            ["user","replyPrivate","session"],
            []
        ],
        "jasy/email/signin":[
            ["util","have","input",[["email","pwd"]]],
            ["email","verifyEmail","input","user"],
            ["email","verifyPwd","input","user"],
            ["util","randomStr",["$sessKey",20]],
            ["user","createSession","cred","user","$sessKey","session"],
            ["user","replyPrivate","session"],
            []
        ],
        "jasy/email/update":[
            ["user","verify","cred"],
            ["email","update","cred","input","output"],
            ["user","replyPrivate","output"],
            []
        ],
        "jasy/user/signout":[
            ["user","verify","cred"],
            ["user","removeSession","cred"],
            []
        ],
        "jasy/user/update":[
            ["user","verify","cred"],
            ["user","update","cred","input","output"],
            ["user","replyPrivate","output"],
            []
        ],
        "jasy/user/list":[
            ["user","verify","cred"],
            ["user","list","input",":output"],
            ["user","replyList",":output"],
            []
        ],
        "jasy/user/read":[
            ["user","verify","cred"],
            ["user","read","input","output"],
            ["user","reply","output"],
            []
        ],
        "jasy/entity/create":[
            ["util","have","input",[["name","type","userId"]]],
            ["user","verify","cred"],
            ["util","randomStr",["$key",20]],
            ["util","randomStr",["$secret",40]],
            ["entity","create","cred","input","$key","$secret","output"],
            ["directory","newGroup","cred","input","output"],
            ["entity","replyPrivate","output"],
            []
        ],
		"jasy/directory/read":[
            ["util","have","input",[["path"]]],
            ["user","verify","cred"],
			["directory","read","input","output"],
            ["directory","replyPrivate","output"],
			[]
		],
        "#help": [
            ["util","help"] 
        ] 
    }
}   
