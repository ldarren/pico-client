{
    "deps":{
        "util":"api/util",
        "user":"api/user",
        "email":"api/email",
        "entity":"api/entity",
        "directory":"api/directory",
		"stream":"api/stream",
        "web":["context","webServer"]
    },      
    "routes":{
        "/jasy":[
            ["util","route","req"],
			["web","parse","req","res"]
        ],
		"/jasy/stream":[
			["web","SSEStart","req","res"],
            ["user","verify","cred"],
			["stream","prepare","cred","input","output"],
			["user","poll","input","output"],
			["directory","poll","cred","input",":poll","output"],
			["directory","last","cred","input",":poll","output"],
			["entity","last","cred","input",":poll","output"],
			["stream","add","res","cred"],
			["stream","render","res",["poll"],"output"]
		],
		"ERR//jasy/stream":[
			["stream","render","res",["error"],"error"],
			["stream","remove","res"]
		],
        "ERR|#msg": [
			["util","logParams"],
            []
        ],
        "END|#msg": [
            [] 
        ],
        "jasy/email/signup":[
            ["util","have","input",[["email","pwd"]]],
            ["email","verifyEmail","input",[null]],
            ["util","randomStr",["$verifyId",20]],
			["entity","getId","cred",["$entId"]],
			["entity","getName","$entId",["$appName"]],
            ["email","sendConfirmation","cred","input","$appName","$verifyId","output"],
            ["user","replyPrivate","output"],
            []
        ],
        "jasy/email/confirm":[
            ["util","have","input",[["email","verifyId"]]],
            ["email","verifyEmail","input",[null]],
            ["email","confirmation","cred","input","user"],
            ["user","create","cred","user"],
            ["util","randomStr",["$sessKey",20]],
			["entity","getId","cred",["$entId"]],
			["directory","getEntityGrp","$entId",["$grp"]],
            ["user","createSession","cred","user","$sessKey","$grp","session"],
            ["user","replyPrivate","session"],
            []
        ],
        "jasy/email/signin":[
            ["util","have","input",[["email","pwd"]]],
            ["email","verifyEmail","input","user"],
            ["email","verifyPwd","input","user"],
            ["util","randomStr",["$sessKey",20]],
			["entity","getId","cred",["$entId"]],
			["directory","getEntityGrp","$entId",["$grp"]],
            ["user","createSession","cred","user","$sessKey","$grp","session"],
            ["user","replyPrivate","session"],
            []
        ],
        "jasy/email/update":[
            ["user","verify","cred"],
            ["email","update","cred","input","output"],
            ["user","replyPrivate","output"],
            []
        ],
        "jasy/user/signout":[
            ["user","verify","cred"],
            ["user","removeSession","cred"],
            []
        ],
        "jasy/user/update":[
            ["user","verify","cred"],
            ["user","update","cred","input","output"],
            ["user","replyPrivate","output"],
            []
        ],
        "jasy/user/list":[
            ["user","verify","cred"],
            ["user","list","input",":output"],
            ["user","replyList",":output"],
            []
        ],
        "jasy/user/read":[
            ["util","have","input",[["id"]]],
            ["user","verify","cred"],
            ["user","read","input","output"],
            ["user","reply","output"],
            []
        ],
        "jasy/entity/create":[
            ["util","have","input",[["name","type","userId"]]],
            ["user","verify","cred"],
            ["util","randomStr",["$key",20]],
            ["util","randomStr",["$secret",40]],
            ["entity","create","cred","input","$key","$secret","output"],
            ["directory","newGroup","cred","input","output"],
            ["entity","replyPrivate","output"],
            []
        ],
		"jasy/directory/read":[
            ["util","have","input",[["cwd"]]],
            ["user","verify","cred"],
			["directory","read","cred","input","output"],
            ["directory","replyPrivate","output"],
			[]
		],
		"jasy/directory/user/new":[
            ["user","verify","cred"],
			["user","read","input","user"],
            ["directory","newUser","cred","user"],
			[]
		],
		"jasy/directory/group/new":[
            ["user","verify","cred"],
			["entity","read","input","entity"],
            ["directory","newGroup","cred","input","entity"],
			[]
		],
		"jasy/directory/group/join":[
            ["user","verify","cred"],
            ["directory","userJoin","cred","input"],
			[]
		],
        "#help": [
            ["util","help"] 
        ] 
    }
}   
