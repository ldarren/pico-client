<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jasaws.pdl.consumer" version="0.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PDL</name>
    <description>
		Put orders online or send message to DD Box sponsored merchants, we will put your orders through DD Box door to door.  You can collect your loved belongings 24/7 any time!
    </description>
    <author email="support@jasaws.com" href="http://jasaws.com">
		Jasa Web Services
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-ble-central" spec="~1.1.0" />
    <plugin name="phonegap-plugin-push" spec="~1.7.0">
        <variable name="SENDER_ID" value="488537087657" />
    </plugin>
    <plugin name="cordova-plugin-device" spec="~1.1.2" />
    <plugin name="cordova-plugin-dialogs" spec="~1.2.1" />
</widget>
