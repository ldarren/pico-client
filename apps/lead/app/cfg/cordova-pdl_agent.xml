<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jasaws.pdl.agent" version="0.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PDL:Agent</name>
	<description>
		Put dirty laundry inside DD Box, send message to DD Box sponsored dry cleaners, we will collect your dirty laundry by next working day, then send back your clean to DD Box once job is done.  You can drop off and collect laundry 24/7 any time!
    </description>
	<author email="support@jasaws.com" href="http://jasaws.com">
		Jasa Web Services
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
	</platform>
	<preference name="DisallowOverscroll" value="true" />
    <engine name="ios" spec="~4.1.1" />
    <engine name="android" spec="~5.1.1" />
    <plugin name="cordova-plugin-ble-central" spec="~1.0.4" />
    <plugin name="phonegap-plugin-push" spec="~1.7.0">
        <variable name="SENDER_ID" value="56885480619" />
    </plugin>
    <plugin name="cordova-plugin-device" spec="~1.1.2" />
</widget>
